<?xml version='1.0' encoding='utf-8'?>
<widget id="se.johancberg.whatsyourweather" version="2.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <name>What's Your Weather</name>
    <description>This is a simple and stylish application that helps you find out information about the current weather.</description>
    <author email="johan94berg@gmail.com" href="http://johancberg.se">
        Johan C Berg
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-navigation href="about:*" />
    <allow-navigation href="index.html" />
    <preference name="loglevel" value="DEBUG" />
</widget>
